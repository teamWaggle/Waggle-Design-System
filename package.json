    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "storybook": "^7.6.4",
    "typescript": "5.0.4",
    "vite": "^4.5.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
